{
  "name": "simple-passwd-manager",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "ts-node --transpile-only ./src/index.ts",
    "build": "tsc --declaration",
    "build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./public/static/css/styles.css",
    "watch": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "watch:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./public/static/css/styles.css --watch"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.7.13",
    "prisma": "^4.2.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
